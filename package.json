{
  "name": "@cocreate/notification",
  "version": "1.4.0",
  "description": "Simple HTML5 & JavaScript component add, update & remove values in element's notification from input, select or js api. Easily configured using HTML5 notification and/or JavaScript API.",
  "keywords": [
    "cocreate",
    "no-code-framework",
    "cocreatejs",
    "cocreatejs-component",
    "cocreate-framework",
    "no-code",
    "low-code",
    "collaborative-framework",
    "realtime",
    "realtime-framework",
    "collaboration",
    "shared-editing",
    "html5-framework",
    "javascript-framework"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npx webpack --config webpack.config.js",
    "build": "npx webpack --mode=production --config webpack.config.js",
    "dev": "npx webpack --config webpack.config.js --watch",
    "postinstall": "node -e \"const { execSync } = require('child_process'); try { execSync('coc --version', { stdio: 'ignore' }); } catch (error) { try { execSync('npm install -g @cocreate/cli', { stdio: 'inherit' }); console.log('Installed \"@cocreate/cli\" globally.'); } catch (error) { console.error('Failed to install \"@cocreate/cli\" globally:', error); } }\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoCreate-app/CoCreate-notification.git"
  },
  "author": "CoCreate LLC",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/CoCreate-app/CoCreate-notification/issues"
  },
  "homepage": "https://cocreate.app/docs/notification",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/CoCreate-app"
  },
  "main": "./src/index.js",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-log": "^3.0.1"
  },
  "dependencies": {
    "@cocreate/actions": "^1.13.0",
    "@cocreate/socket-client": "^1.34.0",
    "web-push": "^3.6.6"
  }
}
